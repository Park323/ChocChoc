{
  "name": "choc-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "description": "An electron app built with vite and react",
  "author": "3SecondsCut",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:ui && npm run build:electron",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.js\"",
    "dev:https": "concurrently \"npm run dev\" \"local-ssl-proxy --source 3001 --target 5173\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:ui": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "electron": "electron dist-electron/main.js",
    "dev:ui": "vite",
    "dev:main": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 tsx electron/main.ts",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:all": "npm run build && electron-builder -mwl"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "styled-components": "^6.1.19",
    "wait-on": "^8.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.3.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.5"
  }
}
